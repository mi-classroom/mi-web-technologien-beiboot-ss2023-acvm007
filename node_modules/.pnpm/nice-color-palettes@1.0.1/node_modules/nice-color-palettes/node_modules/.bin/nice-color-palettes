#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/nice-color-palettes@1.0.1/node_modules/nice-color-palettes/bin/node_modules:/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/nice-color-palettes@1.0.1/node_modules/nice-color-palettes/node_modules:/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/nice-color-palettes@1.0.1/node_modules:/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/nice-color-palettes@1.0.1/node_modules/nice-color-palettes/bin/node_modules:/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/nice-color-palettes@1.0.1/node_modules/nice-color-palettes/node_modules:/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/nice-color-palettes@1.0.1/node_modules:/mnt/d/Coding/Studium/wt-beiboot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
